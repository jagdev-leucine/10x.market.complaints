package model;

/**
 * TriageActions entity.
 */
public class TriageActions {
    private Long id;
    private String actionType;
    private String actionDescription;
    private StaffMembers assignedTo; // Subobject
    private AuditTrails auditTrail; // Subobject
    private String status;

    /**
     * Get the id of the triage action.
     */
    public Long getId() {
        return id;
    }

    /**
     * Set the id of the triage action.
     */
    public void setId(Long id) {
        this.id = id;
    }

    /**
     * Get the type of action.
     */
    public String getActionType() {
        return actionType;
    }

    /**
     * Set the type of action.
     */
    public void setActionType(String actionType) {
        this.actionType = actionType;
    }

    /**
     * Get the description of the action.
     */
    public String getActionDescription() {
        return actionDescription;
    }

    /**
     * Set the description of the action.
     */
    public void setActionDescription(String actionDescription) {
        this.actionDescription = actionDescription;
    }

    /**
     * Get the staff member assigned to the action.
     */
    public StaffMembers getAssignedTo() {
        return assignedTo;
    }

    /**
     * Set the staff member assigned to the action.
     */
    public void setAssignedTo(StaffMembers assignedTo) {
        this.assignedTo = assignedTo;
    }

    /**
     * Get the audit trail for the action.
     */
    public AuditTrails getAuditTrail() {
        return auditTrail;
    }

    /**
     * Set the audit trail for the action.
     */
    public void setAuditTrail(AuditTrails auditTrail) {
        this.auditTrail = auditTrail;
    }

    /**
     * Get the status of the triage action.
     */
    public String getStatus() {
        return status;
    }

    /**
     * Set the status of the triage action.
     */
    public void setStatus(String status) {
        this.status = status;
    }
}